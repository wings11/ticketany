{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "includeFiles": "app/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/$1"
    }
  ]
}
